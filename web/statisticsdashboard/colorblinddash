    <script type="text/javascript">
        $.ajax({
            url: "../../../phpstuff/total_visitor_count.php",
            dataType: "JSON"
        }).success(function(thedata) {
            document.getElementById("tvntid1").innerHTML = thedata.total;
            document.getElementById("uvc").innerHTML = thedata.unique;
            document.getElementById("avg").innerHTML = thedata.average;
            document.getElementById("daily").innerHTML = thedata.dau;
            document.getElementById("xsmall").innerHTML = thedata.xs;
            document.getElementById("small").innerHTML = thedata.sm;
            document.getElementById("medium").innerHTML = thedata.md;
            document.getElementById("large").innerHTML = thedata.la;
            document.getElementById("o").innerHTML = thedata.one.toString().substr(0, 40);
            document.getElementById("t").innerHTML = thedata.two.toString().substr(0, 40);
            document.getElementById("th").innerHTML = thedata.three.toString().substr(0, 40);
            document.getElementById("wc").innerHTML = thedata.windows;
            document.getElementById("lc").innerHTML = thedata.linux;
            document.getElementById("ac").innerHTML = thedata.android;
            document.getElementById("ot").innerHTML = thedata.others;
            new Morris.Donut({
                element: 'morris-donut-chart',
                colors: ['#2f2f2f', '#999999'],
                data: [{
                    label: "Touch Device",
                    value: thedata.Touch
                }, {
                    label: "Non-touch Device",
                    value: thedata.NonTouch
                }],

                resize: true
            });
        });
    </script>

